{
  "name": "myblog",
  "version": "1.0.0",
  "description": "My Development Blog",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack -d && webpack-dev-server --content-base src/ --inline --hot --port 3003 --history-api-fallback --watch",
    "prod": "npm run clean && cross-env NODE_ENV=production webpack -p",
    "dev:old": "webpack -d && copy src\\index.html dist && webpack-dev-server --content-base src/ --inline --hot --port 3003 --history-api-fallback",
    "prod:old": "webpack -p && copy src\\index.html dist",
    "copy": "copy src\\index.html dist",
    "clean": "rimraf ./dist/*"
  },
  "keywords": [
    "Programming",
    "Development",
    "Blog"
  ],
  "author": "Benny Chen",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.0.0-beta",
    "install": "^0.10.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "npm": "^5.3.0",
    "popper.js": "^1.12.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  }
}
